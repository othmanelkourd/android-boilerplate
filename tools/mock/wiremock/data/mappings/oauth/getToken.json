{
    "request": {
        "method": "POST",
        "url": "/oauth/token"
    },
    "response": {
        "status": 200,
        "headers": {
            "Content-Type": "application/json"
        },
        "jsonBody": {
            "token_type": "Bearer",
            "access_token": "MOCK_ACCESS_TOKEN",
            "refresh_token": "MOCK_REFRESH_TOKEN",
            "expires_in": "500000",
            "scope": ["admin"],
            "state": ""
        }
    }
}