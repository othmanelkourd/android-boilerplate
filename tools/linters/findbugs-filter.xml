<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>

	<Match>
		<Class name="~.*\.R\$.*" />
	</Match>
	<Match>
		<Class name="~.*\.Manifest\$.*" />
	</Match>
	<Match>
		<Method name="IfRequestMatches" />
		<Bug code="RV_RETURN_VALUE_IGNORED_NO_SIDE_EFFECT" />
	</Match>
	<Match>
		<Class name="~.*\.*" />
		<Bug pattern="DM_DEFAULT_ENCODING" />
	</Match>
	<Match>
		<Class name="~.*Test$" />
	</Match>
	<Match>
		<Package name="~test\..*" />
	</Match>
	<Match>
		<Class name="~.*\.*Test" />
		<Not>
			<Bug code="IJU" />
		</Not>
	</Match>

	<!-- DTOs -->
	<Match>
		<Package name="~.*\.dtos\..*" />
	</Match>

</FindBugsFilter>
